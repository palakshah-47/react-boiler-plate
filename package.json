{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "coverageReporters": [
      "text",
      "json",
      "lcov",
      "clover",
      "cobertura"
    ],
    "collectCoverageFrom": [
      "src/reducers/**/*.js",
      "src/util/**/*.js",
      "!src/util/index.js",
      "!src/util/global.js"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.8.3",
    "axios": "0.21.1",
    "axios-mock-adapter": "1.19.0",
    "babel-jest": "^26.6.0",
    "classnames": "2.3.1",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-jest": "24.3.2",
    "gcui": "file:react-0.3.1.tgz",
    "moment": "2.29.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "storybook-addon-material-ui": "0.9.0-alpha.24",
    "web-vitals": "1.1.0"
  },
  "scripts": {
    "build:run": "cd build && http-server --push-state",
    "build:dev": "REACT_APP_ENV=dev react-scripts build",
    "build:prod": "cross-env REACT_APP_ENV=prod react-scripts build",
    "build-storybook": "build-storybook -s public",
    "eject": "react-scripts eject",
    "lintj": "./node_modules/.bin/eslint --ext .js src/",
    "start": "cross-env DISABLE_ESLINT_PLUGIN=true REACT_APP_ENV=local npm-run-all -p start-js",
    "start-js": "react-scripts start",
    "start-mock": "cross-env DISABLE_ESLINT_PLUGIN=true REACT_APP_ENV=mock npm-run-all -p start-js",
    "test": "cross-env REACT_APP_ENV=unitTest CI=true react-scripts test --setupFiles ./src/setup-tests.js --colors --env=jsdom --coverage",
    "storybook": "start-storybook -p 6006 -c .storybook watch-css -s ./src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not chrome < 51",
      "not dead",
      "not ie 11",
      "not op_mini all",
      "not safari < 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.1.21",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/addon-links": "6.1.21",
    "@storybook/node-logger": "6.1.21",
    "@storybook/preset-create-react-app": "3.1.7",
    "@storybook/react": "6.1.21",
    "cross-env": "7.0.3",
    "eslint": "7.22.0",
    "eslint-plugin-react": "7.22.0",
    "npm-run-all": "4.1.5",
    "redux-mock-store": "1.5.4"
  }
}
